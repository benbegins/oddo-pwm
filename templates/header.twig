<div v-scope="VersionSelection()">
    <header v-scope="Menu()" @vue:mounted="scrollEvent($el)" class="site-header z-30 top-0 w-full fixed transition-all duration-500{% if hero_light %} is_dark{% endif %}" :class="{'is-open': menuOpen}">
        <div class="hidden lg:block bg-greige relative z-20">
            <div class="container-xl flex justify-end gap-6 py-1 px-6 text-xs">
                <div><a href="{{page_contact}}" class="hover:text-rouge">{{ __('Contactez-nous', 'bemy') }}</a></div>
                <div><a href="{{page_agences}}" class="hover:text-rouge">{{ __('Trouvez-nous', 'bemy') }}</a></div>
                {% set post_translations = function('pll_get_post_translations', post.id) %}
                {% if post_translations %}
                    <div>
                        <button class="flex gap-2" @click="openPopin">
                            {% for lang, id in post_translations %}
                                {% if id == post.id %}
                                    <span class="text-rouge">{{lang|capitalize}}</span>
                                {% else %}
                                    <span class="opacity-40">{{lang|capitalize}}</span>
                                {% endif %}
                            {% endfor %}
                        </button>
                    </div>
                {% endif %}
            </div>
        </div>

        <div class="container-xl lg:flex lg:justify-between lg:items-center">
            <div class="site-header__logo-burger  transition-colors duration-500 relative z-20 flex justify-between px-5 py-3 lg:px-6">
